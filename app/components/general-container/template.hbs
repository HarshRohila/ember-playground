<LinkTo @route='cart' class='cart-link'>
  <i class='fa fa-shopping-cart'></i>
  {{!-- {{log this.shoppingCartService.items.length}} --}}
  {{!-- {{log this.shoppingCartService.totalItems}} --}}

  <span
    class='cart-count d-inline-flex justify-content-center align-items-center'
  >
    {{this.shoppingCartService.totalCount}}
  </span>
</LinkTo>
<main class='container mt-5'>
  {{yield}}
</main>